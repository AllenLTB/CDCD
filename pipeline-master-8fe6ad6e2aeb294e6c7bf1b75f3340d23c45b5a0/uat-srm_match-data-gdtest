node () {
def workspace = pwd()
def branch = 'master'




 stage 'checkout'
	dir('common-kafka'){
          git branch: branch , credentialsId: 'jenkins', url: 'http://47.94.250.239/SRM/common-kafka.git'
    }  
	dir('invoke-bus-server-api'){
          git branch: branch , credentialsId: 'jenkins', url: 'http://47.94.250.239/gd_paybalance_dev/invoke-bus-server-api.git'
    }
     dir('match-data'){
          git branch: 'test' , credentialsId: 'jenkins', url: 'http://47.94.250.239/SRM/match-data.git'
    }



stage 'Build'
    sh"""
	   cd $workspace/common-kafka
       /usr/local/maven/bin/mvn clean install
	   cd $workspace/invoke-bus-server-api
       /usr/local/maven/bin/mvn clean install 
       cd $workspace/match-data
       /usr/local/maven/bin/mvn clean install -Ptest
         """
stage 'PRE'
    sh"""
    rm -rf /var/lib/jenkins/workspace/uat-srm_match-data-step1-gdtest/*
    cp -f $workspace/match-data/match-data-server/target/match-data-server.war /var/lib/jenkins/workspace/uat-srm_match-data-step1-gdtest/
    """
}